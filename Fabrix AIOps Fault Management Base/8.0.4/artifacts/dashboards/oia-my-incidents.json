{
    "name": "oia-my-incidents",
    "label": "My Incidents",
    "description": "My Incidents",
    "dashboard_style": "auto",
    "enabled": true,
    "stream": "oia-incidents-stream",
    "dashboard_type": "template",
    "template_variables": {
        "PROJECT_ID": {
            "default": "d2b1d314-99f1-495c-8315-ff4ccacc02c3",
            "contextId": "USER_PROJECTS"
        },
        "USERID": {
            "default": "acme@cfx.com",
            "contextId": "USER_ID"
        }
    },
    "status_poller": {
        "stream": "oia-incidents-stream",
        "frequency": 15,
        "columns": [
            "timestamp"
        ],
        "sorting": [
            {
                "timestamp": "desc"
            }
        ],
        "query": "timestamp is after '$timestamp'",
        "defaults": {
            "timestamp": "$UTCNOW"
        },
        "action": "refresh"
    },
    "dashboard_filters": {
        "time_filter": true,
        "extra_filter": "i_assigned_to is '{{USERID}}' ",
        "default_time_filter_non_removable": true,
        "show_default_time_filter": false,
        "additional_datetime_column_filters": [
            {
                "id": "i_created_ts",
                "label": "Created At"
            },
            {
                "id": "i_updated_ts",
                "label": "Updated At"
            }
        ],
        "columns_filter": [
            {
                "id": "incident_id",
                "label": "Incident Id",
                "type": "TEXT"
            },
            {
                "id": "customer_id",
                "label": "Customer Id",
                "type": "TEXT"
            },
            {
                "id": "project_id",
                "label": "Organizations",
                "type": "TEXT"
            },
            {
                "id": "i_summary",
                "label": "Summary",
                "type": "TEXT"
            },
            {
                "id": "i_description",
                "label": "Description",
                "type": "TEXT"
            },
            {
                "id": "i_priority_label",
                "label": "Priority",
                "type": "TEXT"
            },
            {
                "id": "i_state",
                "label": "State",
                "type": "TEXT"
            },
            {
                "id": "i_starred",
                "label": "Starred",
                "type": "TEXT"
            },
            {
                "id": "i_alert_sources",
                "label": "Alert Sources",
                "type": "TEXT"
            },
            {
                "id": "i_source_key",
                "label": "Source Key",
                "type": "TEXT"
            },
            {
                "id": "i_created_ts",
                "label": "Created",
                "type": "DATETIME"
            },
            {
                "id": "i_updated_ts",
                "label": "Updated",
                "type": "DATETIME"
            },
            {
                "id": "i_resolved_at_ts",
                "label": "Resolved At",
                "type": "DATETIME"
            },
            {
                "id": "i_closed_at_ts",
                "label": "Closed At",
                "type": "DATETIME"
            },
            {
                "id": "i_assigned_to",
                "label": "Assigned To",
                "type": "TEXT"
            },
            {
                "id": "i_resolved_by",
                "label": "Resolved By",
                "type": "TEXT"
            },
            {
                "id": "i_resolution_code",
                "label": "Resolution Code",
                "type": "TEXT"
            },
            {
                "id": "i_assigned_to",
                "label": "Assigned To",
                "type": "TEXT"
            },
            {
                "id": "i_updated_by",
                "label": "Updated By",
                "type": "TEXT"
            },
            {
                "id": "i_reported_by",
                "label": "Reported By",
                "type": "TEXT"
            },
            {
                "id": "i_created_by",
                "label": "Created By",
                "type": "TEXT"
            },
            {
                "id": "i_closed_by",
                "label": "Closed By",
                "type": "TEXT"
            },
            {
                "id": "i_tags",
                "label": "Tags",
                "type": "TEXT"
            }
        ],
        "group_filters": [
            {
                "stream": "oia-incidents-stream",
                "title": "Alert Sources",
                "group_by": [
                    "i_alert_sources"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "incident_id",
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Assigned To",
                "group_by": [
                    "i_assigned_to"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "incident_id",
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Priority",
                "group_by": [
                    "i_priority_label"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "incident_id",
                "htmlTemplateForRow": "<span class='badge' style='background-color: {{'{{ row.i_priority_color }}'}}' > {{'{{ row.i_priority }}'}} - {{'{{ row.i_priority_label }}'}} </span>",
                "duration_hours": 24,
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "State",
                "group_by": [
                    "i_state"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "incident_id",
                "duration_hours": 24,
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Starred",
                "group_by": [
                    "i_starred"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "incident_id",
                "duration_hours": 24,
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Organizations",
                "group_by": [
                    "project_id"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "project_id",
                "type": "str"
            }
        ],
        "default_time_filter_labels": [
            "Last 24 hours"
        ]
    },
    "import": [
        {
            "from": "oia-incidents-os-template",
            "as": "incidents"
        }
    ],
    "dashboard_sections": [
        {
            "title": "Incidents",
            "show_filter": true,
            "projects": [
                "ACME"
            ],
            "scope_filter": "i_assigned_to is '{{USERID}}' ",
            "widgets": [
                {
                  "extends": "self:incident_by_priority_pie_chart",
                  "widget_id": "94a3fc11"
                },
                {
                  "extends": "self:assigned_to_pie_chart",
                  "widget_id": "94a3fc11"
                },
                {
                  "extends": "self:incident_by_status_pie_chart",
                  "widget_id": "94a3fc11"
                },
                {
                    "extends": [
                        "incidents:incident_tabular",
                        "incidents:actions_lib.incidents_tabular_actions"
                    ],
                    "min_width": 12,
                    "height": 10,
                    "max_width": 12,
                    "extend_vars": {
                        "incidents_tabular_extra_filter": "project_id in {{PROJECT_ID}}"
                    },
                    "widget_id": "94a3fc11"
                }

            ]
        }
    ],
    "library": {
    "widgets": {
      "incident_by_priority_pie_chart": {
                    "widget_type": "pie_chart",
                    "title": "Incidents By Priority",
                    "stream": "oia-incidents-stream",
                    "ts_column": "timestamp",
                    "min_width": 4,
                    "duration_hours": 24,
                    "height": 4,
                    "max_width": 4,
                    "column": "incident_id",
                    "agg": "value_count",
                    "group_by": [
                        "i_priority_label"
                    ],
                    "style": {
                        "color-map": {
                            "Critical": [
                                "#B71C1C",
                                "#ffffff"
                            ],
                            "High": [
                                "#FFA726",
                                "#ffffff"
                            ],
                            "Moderate": [
                                "#90CAF9",
                                "#ffffff"
                            ],
                            "Low": [
                                "#4DD0E1",
                                "#ffffff"
                            ],
                            "Planning": [
                                "#4FC3F7",
                                "#ffffff"
                            ]
                        }
                    },
                    "type": "str",
                    "widget_id": "cb0706e6"
                },
      "assigned_to_pie_chart": {
                    "widget_type": "pie_chart",
                    "title": "Assigned To",
                    "stream": "oia-incidents-stream",
                    "ts_column": "timestamp",
                    "min_width": 4,
                    "duration_hours": 24,
                    "height": 4,
                    "column": "incident_id",
                    "agg": "value_count",
                    "group_by": [
                        "i_assigned_to"
                    ],
                    "type": "str",
                    "widget_id": "cb0706e6"
                },
        "incident_by_status_pie_chart": {
                    "widget_type": "pie_chart",
                    "title": "Incidents By Status",
                    "stream": "oia-incidents-stream",
                    "ts_column": "timestamp",
                    "min_width": 4,
                    "duration_hours": 24,
                    "height": 4,
                    "column": "incident_id",
                    "agg": "value_count",
                    "group_by": [
                        "i_state"
                    ],
                    "type": "str",
                    "style": {
                        "color-map": {
                            "Resolved": [
                                "#4CAF50",
                                "#ffffff"
                            ],
                            "Assigned": [
                                "#FFA726",
                                "#ffffff"
                            ],
                            "Open": [
                                "#B71C1C",
                                "#ffffff"
                            ],
                            "On Hold": [
                                "#FFA726",
                                "#ffffff"
                            ],
                            "Cancelled": [
                                "#4CAF50",
                                "#ffffff"
                            ],
                            "Closed": [
                                "#4CAF50",
                                "#ffffff"
                            ]
                        }
                    },
                    "widget_id": "cb0706e6"
                }
    }
  },
    "saved_time": "2023-09-12T10:15:31.554433"
}