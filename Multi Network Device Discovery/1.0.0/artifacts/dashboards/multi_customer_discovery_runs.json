                                                        "max_width": 12,
                                                        "height": 8,
                                                        "min_width": 12,
                                                        "extra_filter0": "type contains 'windows'",
                                                        "stream": "multi_discovery",
                                                        "extra_filter": "(customer_tag contains '{{CUST_TAG}}' or customer_tag is empty) and network_name is '{{NETWORK_NAME}}'",
