{
    "name": "self_monitoring",
    "label": "Self Monitoring",
    "description": "Host OS Performance Metrics Dashboard",
    "version": "2024-05-23_1",
    "enabled": true,
    "live_edit": true,
    "dashboard_style": "tabbed",
    "dashboard_folder": "Host_OS",
    "dashboard_type": "dashboard",
    "custom_context": {
        "rda-dashboard-live-edit": true
    },
    "status_poller": {
        "stream": "host_os_metrics",
        "frequency": 600,
        "columns": [
            "timestamp"
        ],
        "sorting": [
            {
                "timestamp": "desc"
            }
        ],
        "query": "timestamp is after '${timestamp}'",
        "defaults": {
            "timestamp": "$UTCNOW"
        },
        "action": "refresh"
    },
    "dashboard_filters": {
        "time_filter": true,
        "default_time_filter_labels": [
            "Last 24 hours"
        ],
        "columns_filter": [
            {
                "id": "timestamp",
                "label": "Timestamp",
                "type": "DATETIME"
            },
            {
                "id": "i_deployment",
                "label": "RDAF Deployment",
                "type": "TEXT"
            },
            {
                "id": "name",
                "label": "Metric Name",
                "type": "TEXT"
            },
            {
                "id": "i_container_name",
                "label": "Container Name",
                "type": "TEXT"
            },
            {
                "id": "i_host_ip",
                "label": "Host IP",
                "type": "TEXT"
            },
            {
                "id": "i_host",
                "label": "Hostname",
                "type": "TEXT"
            },
            {
                "id": "PARTITION",
                "label": "Partition",
                "type": "int"
            }
        ],
        "group_filters": [
            {
                "stream": "host_os_metrics",
                "title": "RDAF Deployment",
                "limit": 50,
                "group_by": [
                    "i_deployment"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "i_deployment",
                "type": "int",
                "show_counts": false
            },
            {
                "stream": "host_os_metrics",
                "title": "Hostname",
                "limit": 50,
                "group_by": [
                    "i_host"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "i_host",
                "type": "int",
                "show_counts": false
            },
            {
                "stream": "host_os_metrics",
                "title": "Host IP",
                "limit": 50,
                "group_by": [
                    "i_host_ip"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "i_host_ip",
                "type": "str",
                "show_counts": false
            },
            {
                "stream": "host_os_metrics",
                "title": "Metric Name",
                "limit": 50,
                "group_by": [
                    "name"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "name",
                "type": "str",
                "show_counts": false
            },
            {
                "stream": "host_os_metrics",
                "title": "Device Name",
                "limit": 50,
                "group_by": [
                    "i_device"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "name",
                "type": "str",
                "show_counts": false
            },
            {
                "stream": "host_os_metrics",
                "title": "Tag Device",
                "limit": 50,
                "group_by": [
                    "i_device"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "count_",
                "type": "int",
                "show_counts": false
            },
            {
                "stream": "bulk_stat_ingestion_status",
                "title": "Partition",
                "limit": 50,
                "group_by": [
                    "PARTITION"
                ],
                "ts_column": "timestamp",
                "agg": "cardinality",
                "column": "count_",
                "type": "int",
                "show_counts": false
            }
        ]
    },
