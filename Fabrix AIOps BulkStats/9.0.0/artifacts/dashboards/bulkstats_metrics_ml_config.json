                "agg": "cardinality",
                "column": "_RDA_Id",
                "type": "int"
            },
            {
                "stream": "ml-bulkstats-metrics-variance",
                "extra_filter": "customer_tag is '{{CUST_TAG}}' or customer_tag is empty",
                "title": "Statistically Selected",
                "duration_hours": 1000,
                "group_by": [
                    "statistically_selected"
