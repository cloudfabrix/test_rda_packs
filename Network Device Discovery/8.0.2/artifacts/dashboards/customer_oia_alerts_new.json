    "dashboard_sections": [
        {
            "title": "Alerts",
            "show_filter": true,
            "widgets": [
                {
                    "widget_type": "label",
                    "max_width": 12,
                    "height": 0.5,
                    "min_width": 12,
                    "label": "<div style='text-align:center;width:100%;'><h1><a style='text-decoration:none;color:#0066ff;' target='_blank'>Customer Alerts</a></h1></div>",
                    "widget_id": "33317e49"
                },
                {
                    "title": "Active Alerts",
                    "widget_type": "custom_counter",
                    "stream": "oia-alerts-stream",
                    "ts_column": "a_updated_ts",
                    "extra_filter": "a_status is 'ACTIVE'",
                    "column": "count_",
                    "agg": "sum",
                    "type": "str",
                    "style": {
                        "color-list": [
                            "#FFA726"
                        ]
                    },
                    "sparkline": {
                        "interval": "4h"
                    },
                    "max_width": 2.4,
                    "height": 4,
                    "min_width": 2.4,
                    "widget_id": "90fc33be"
                },
                {
                    "title": "Alerts Suppressed",
                    "widget_type": "custom_counter",
                    "stream": "oia-alerts-stream",
                    "extra_filter": "a_correlation_status in ['SUPPRESSED']",
                    "ts_column": "a_updated_ts",
                    "column": "count_",
                    "agg": "sum",
                    "type": "int",
                    "min_width": 2.4,
                    "height": 4,
                    "max_width": 2.4,
                    "style": {
                        "color-list": [
                            "#4CAF50"
                        ]
                    },
                    "sparkline": {
                        "interval": "4h"
                    },
                    "widget_id": "02be1e23"
                },
                {
                    "title": "Cleared Alerts",
                    "widget_type": "custom_counter",
                    "stream": "oia-alerts-stream",
                    "extra_filter": " a_status is 'CLEARED'",
                    "ts_column": "a_updated_ts",
                    "column": "count_",
                    "max_width": 2.4,
                    "height": 4,
                    "min_width": 2.4,
                    "style": {
                        "color-list": [
                            "#4CAF50"
                        ]
                    },
                    "sparkline": {
                        "interval": "4h"
                    },
                    "agg": "sum",
                    "type": "str",
                    "widget_id": "53383e25"
                },
                {
                    "widget_id": "02be1e21",
                    "title": "Alerts Deduped",
                    "min_width": 2.4,
                    "height": 4,
                    "max_width": 2.4,
                    "widget_type": "expression_counter",
                    "expression": "de_totalRepeatCount - de_alertsWithRepeatCountMoreThanOne",
                    "unit": "",
                    "segments": [
                        {
                            "variable": "de_totalRepeatCount",
                            "stream": "oia-alerts-stream",
                            "extra_filter": "a_repeat_count >= 1",
                            "ts_column": "a_updated_ts",
                            "column": "a_repeat_count",
                            "agg": "sum",
                            "type": "int"
                        },
                        {
                            "variable": "de_alertsWithRepeatCountMoreThanOne",
                            "stream": "oia-alerts-stream",
                            "extra_filter": "a_repeat_count >= 1",
                            "ts_column": "a_updated_ts",
                            "column": "count_",
                            "agg": "sum",
                            "type": "int"
                        }
                    ]
                },
                {
                    "title": "Alerts Ingested",
                    "widget_type": "custom_counter",
                    "stream": "oia-alerts-stream",
                    "extra_filter0": "a_source_systemname is not 'Alert Group' and a_repeat_count >= 1 ",
                    "extra_filter2": "a_en_customer_tag contains '{{CUST_TAG}}'",
                    "ts_column": "a_updated_ts",
                    "column": "a_repeat_count",
                    "max_width": 2.4,
                    "height": 4,
                    "min_width": 2.4,
                    "agg": "sum",
                    "type": "int",
                    "style": {
                        "color-list": [
                            "#1E88E5"
                        ]
                    },
                    "sparkline": {
                        "interval": "4h"
                    },
                    "widget_id": "e579e213"
                },
                {
                    "widget_type": "pie_chart",
                    "title": "Alerts by State",
                    "description": "Does not include duplicates",
                    "stream": "oia-alerts-stream",
                    "extra_filter0": "a_source_systemname is not 'Alert Group'",
                    "extra_filter1": "a_en_customer_tag contains '{{CUST_TAG}}'",
                    "ts_column": "a_updated_ts",
                    "column": "count_",
                    "style": {
                        "color-map": {
                            "CLEARED": [
                                "#4CAF50",
                                "#ffffff"
                            ],
                            "ACTIVE": [
                                "#FFA726",
                                "#ffffff"
                            ]
                        }
                    },
                    "max_width": 3,
                    "height": 4,
                    "min_width": 3,
                    "agg": "sum",
                    "group_by": [
                        "a_status"
                    ],
                    "type": "int",
                    "widget_id": "c9ac6704"
                },
                {
                    "title": "Active Alerts by Devices",
                    "widget_type": "pie_chart",
                    "stream": "oia-alerts-stream",
                    "extra_filter": "a_status is 'ACTIVE'",
                    "ts_column": "a_updated_ts",
                    "segment_filter": false,
                    "column": "count_",
                    "group_by": [
                        "a_asset_ip_address"
                    ],
                    "agg": "sum",
                    "max_width": 3,
                    "height": 4,
                    "min_width": 3,
                    "widget_id": "5b9bd1c8"
                },
                {
                    "widget_type": "multi_bar_chart",
                    "title": "Active Alerts by Source and Severity",
                    "stream": "oia-alerts-stream",
                    "ts_column": "a_updated_ts",
                    "column": "count_",
                    "description": "Values: Severe, Critical, Major, Minor, Warning, Notification, Information",
                    "extra_filter": "a_status not in ['CLEARED']",
                    "segment_filter": false,
                    "max_width": 6,
                    "height": 6,
                    "min_width": 6,
                    "style": {
                        "color-map": {
                            "SEVERE": "#B71C1C",
                            "CRITICAL": "#E53935",
                            "MAJOR": "#F57C00",
                            "MINOR": "#FFA726",
                            "WARNING": "#FFD54F",
                            "NOTIFICATION": "#4DD0E1",
                            "INFORMATION": "#03A9F4"
                        }
                    },
                    "chartProperties": {
                        "yAxisLabel": "Source",
                        "xAxisLabel": "Count",
                        "stacked": true,
                        "legendLocation": "right",
                        "orientation": "horizontal"
                    },
                    "agg": "sum",
                    "group_by": [
                        "a_severity",
                        "a_source_systemname"
