                "ts_column": "a_updated_ts",
                "agg": "sum",
                "column": "count_",
                "type": "str"
            },
            {
                "stream": "oia-alerts-stream",
                "title": "Monitoring Status",
                "group_by": [
                    "a_status"
                ],
                "ts_column": "a_updated_ts",
                "extra_filter": "a_source_systemname is not 'Alert Group'",
                "agg": "sum",
                "column": "count_",
                "type": "str"
            },
            {
                "stream": "oia-alerts-stream",
                "title": "Severity",
                "group_by": [
                    "a_severity"
