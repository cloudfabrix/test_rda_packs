                    "title": "Asset Hierarchy",
                    "widget_type": "device_tree_view",
                    "max_width": 6,
                    "height": 6,
                    "min_width": 6,
                    "stream": "network_device_inventory",
                    "extra_filter": "(customer_id is '{{CUST_ID}}') and (parent_sn in '{{SERIAL_NUM}}')",
                    "ts_column": "timestamp",
                    "duration_hours": 1080,
                    "widget_id": "e77160db",
                    "context_filter": "org_id not in ['','Not Available']"
                },
                {
                    "title": "Asset Ops View",
                    "widget_type": "device_ops_view",
                    "max_width": 6,
                    "height": 6,
                    "min_width": 6,
                    "stream": "network_device_inventory",
                    "extra_filter": "(customer_id is '{{CUST_ID}}') and (parent_sn in '{{SERIAL_NUM}}')",
                    "ts_column": "timestamp",
                    "duration_hours": 1080,
                    "widget_id": "9a4be5df",
                    "context_filter": "org_id not in ['','Not Available']"
                },
                {
                    "title": "Interface Status",
                    "widget_type": "tabular",
                    "stream": "network_device_interfaces",
                    "extra_filter": "(device_ip is '{{DEVICE_IP}}' or device_serial_number is '{{SERIAL_NUM}}') and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty)",
                    "max_width": 12,
                    "height": 8,
                    "min_width": 12,
                    "remote_searchable": true,
                    "remote_searchable_cols": [
                        "device_ip",
                        "device_hostname",
                        "device_int_name",
                        "ifTypeLabel"
                    ],
                    "columns": {
