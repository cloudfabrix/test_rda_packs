{
    "name": "device_inventory_from_alerts",
    "label": "Inventory",
    "description": "Device Inventory",
    "dashboard_folder": "NetworkDevicePack",
    "version": "24.05.20.2",
    "enabled": false,
    "stream": "network_device_inventory",
    "dashboard_type": "template",
    "live_edit": true,
    "template_variables": {
        "CUST_ID": {
            "contextId": "customerId"
        },
        "PROJ_ID": {
            "contextId": "project_id"
        },
        "PROJ_ID_SINGLE": {
            "contextId": "project_id"
        },
        "CUST_TAG": {
            "contextId": "customer_tag"
        },
        "SERIAL_NUM": {
            "contextId": "parent_sn"
        },
        "DEVICE_IP": {
            "contextId": "device_ip"
        }
    },
    "computedContext0": {
        "org_id": {
            "expr": "'single_tenant' if customer_id is empty else customer_id",
            "defaultValue": "single_tenant"
        }
    },
    "dashboard_filters": {
        "time_filter": true,
        "extra_filter0": "(device_ip is '{{DEVICE_IP}}') and  (customer_tag is '{{CUST_TAG}}' or customer_tag is empty)",
        "extra_filter": "device_ip is '{{DEVICE_IP}}'",
        "default_time_filter_labels": [
            "Last 1 month"
        ],
        "columns_filter": [
