    "dashboard_sections": [
        {
            "title": "Device View",
            "show_filter": true,
            "widgets": [
                {
                    "title": "Asset Summary",
                    "widget_type": "summary",
                    "max_width": 6,
                    "height": 5,
                    "min_width": 6,
                    "stream": "network_device_inventory",
                    "duration_hours": 1080,
                    "ts_column": "timestamp",
                    "extra_filter": "(parent_sn in '{{SERIAL_NUM}}') and device_equip_type contains 'chassis' and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty)",
                    "columns": {
                        "device_model": "Product ID",
                        "device_equip_type": "Equipment Type",
                        "device_vendor": "Vendor",
                        "device_ip": "IP Address",
                        "device_hostname": "Hostname",
                        "parent_sn": "Parent Serial Number"
                    },
                    "widget_id": "63ea4c7f"
                },
                {
                    "title": "Asset Additional Details",
                    "widget_type": "summary",
                    "max_width": 6,
                    "height": 5,
                    "min_width": 6,
                    "stream": "network_device_inventory",
                    "ts_column": "timestamp",
                    "duration_hours": 1080,
                    "extra_filter0": "(device_ip is '{{DEVICE_IP}}' or parent_sn is '{{SERIAL_NUM}}') and device_equip_type in ['chassis','CHASSIS']",
                    "extra_filter1": "(customer_id is '{{CUST_ID}}') and (project_id in {{PROJ_ID}}) and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty) and (parent_sn in '{{SERIAL_NUM}}') and device_equip_type contains 'chassis'",
                    "extra_filter": "(parent_sn in '{{SERIAL_NUM}}') and device_equip_type contains 'chassis' and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty)",
                    "columns": {
