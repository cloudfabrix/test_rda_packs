{
    "name": "customer_access_status",
    "description": "Customer Access Status",
    "label": "Failed Devices",
    "enabled": false,
    "dashboard_type": "template",
    "dashboard_folder": "NetworkDevicePack",
    "version": "24.9.6.3",
    "template_variables": {
        "CUST_ID": {
            "contextId": "customerId"
        },
        "PROJ_ID": {
            "contextId": "projectId"
        },
        "CUST_TAG": {
            "contextId": "customerTag"
        }
    },
    "dashboard_filters": {
