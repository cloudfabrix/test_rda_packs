                    "min_width": 6,
                    "height": 4,
                    "agg": "value_count",
                    "group_by": [
                        "auth_state"
                    ],
                    "style": {
                        "color-map": {
                            "Success": [
                                "#008000",
                                "#ffffff"
                            ],
                            "Failed": [
