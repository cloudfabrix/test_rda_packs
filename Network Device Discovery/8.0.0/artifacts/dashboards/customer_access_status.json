            {
                "stream": "network_access_verification",
                "title": "Protocol",
                "group_by": [
                    "protocol"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "type": "str"
            },
            {
                "stream": "network_access_verification",
                "title": "Credential Name",
                "group_by": [
                    "used_cred"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "type": "str"
            },
            {
                "stream": "network_access_verification",
                "title": "Authentication State",
                "group_by": [
                    "auth_state"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "type": "str"
            }
        ],
        "columns_filter": [
            {
                "id": "protocol",
                "label": "Protocol",
                "type": "TEXT"
            },
            {
                "id": "ip_address",
                "label": "IP Address",
                "type": "TEXT"
            },
            {
                "id": "used_cred",
                "label": "Credential Name",
                "type": "TEXT"
            },
            {
