{
    "name": "customer_oia_incidents",
    "label": "Incidents Dashboard",
    "description": "Incidents Dashboard",
    "version": "2023.12.17",
    "enabled": false,
    "dashboard_style": "auto",
    "dashboard_folder": "MSP",
    "dashboard_type": "template",
    "stream": "oia-incidents-stream",
    "stream_query_mapping": {
        "oia-alerts-stream": {
            "timestamp": "a_created_ts",
            "i_priority_label": null
        },
        "oia-incidents-stream": {
            "timestamp": "i_created_ts",
            "i_priority_label1": null
        }
    },
    "template_variables": {
        "CUST_ID": {
            "contextId": "customerId"
        },
        "PROJ_ID": {
            "contextId": "projectId"
        },
        "PROJ_ID_SINGLE": {
            "contextId": "project_id"
        },
        "CUST_TAG": {
            "contextId": "customerTag"
        }
    },
    "dashboard_filters": {
        "time_filter": true,
        "extra_filter0": "customer_id is '{{CUST_ID}}' and attrs_customerTag is '{{CUST_TAG}}'",
        "extra_filter1": "project_id is '{{PROJ_ID_SINGLE}}'  and attrs_customerTag is '{{CUST_TAG}}'",
        "default_time_filter_labels": [
            "Last 1 month"
        ],
        "group_filters": [
            {
                "stream": "oia-incidents-stream",
                "title": "Alert Sources",
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "group_by": [
                    "i_alert_sources"
                ],
                "ts_column": "timestamp",
                "agg": "sum",
                "column": "count_",
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Assigned To",
                "group_by": [
                    "i_assigned_to"
                ],
                "ts_column": "timestamp",
                "agg": "sum",
                "column": "count_",
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Priority",
                "group_by": [
                    "i_priority_label"
                ],
                "ts_column": "timestamp",
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "agg": "sum",
                "column": "count_",
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "State",
                "group_by": [
                    "i_state"
                ],
                "ts_column": "timestamp",
                "agg": "sum",
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "column": "count_",
                "duration_hours": 24,
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Starred",
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "group_by": [
                    "i_starred"
                ],
                "ts_column": "timestamp",
                "agg": "sum",
                "column": "count_",
                "duration_hours": 24,
                "type": "str"
            },
            {
                "stream": "oia-incidents-stream",
                "title": "Device",
                "group_by": [
                    "attrs_alert-asset-name"
                ],
                "extra_filter": "i_state does not contains 'cancelled'  AND attrs_network_type in ['FirstNet','Consumer','Enterprise']",
                "ts_column": "timestamp",
